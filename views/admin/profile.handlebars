<div>
    <a href="/" class="main">Главная</a>
    -> <a href="/admin" class="main">Админ</a>
    <span class="main"> -> Профайл</span>
</div>

{{#if profile}}
    <ul>
        {{#each profile}}
            <li class="admin__profile_title">
                <a href="/admin/profile/{{this._id}}">{{this.name}}</a>
                <form method="GET" action="/admin/profile/{{this._id}}/delete">
                    <button class="delete_button">Delete</button>
                </form>
            </li>
        {{/each}}
    </ul>
{{/if}}

<div>
    {{#each pages}}
        <a href="/admin?page={{this}}">{{this}}</a>
    {{/each}}
</div>

